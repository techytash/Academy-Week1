package introtojava;

public class ASCIIExample {

	public static void main(String[] args) {
		// In main method, create a variable of type char 
		  char c = 'N';
		  
		// Assign created variable in step# 2 to another variable of type int 
		  int ascii = N;
		  
		//Print the variable of the type int created in step#3.
		  System.out.println("The ASCII value of " + N + " is: " + ascii);

